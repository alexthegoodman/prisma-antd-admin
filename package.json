{
  "name": "prisma-antd-admin",
  "version": "1.0.0",
  "description": "Admin Panel for Prisma / Nexus leveraging Ant Design",
  "main": "index.ts",
  "repository": "https://github.com/alexthegoodman/prisma-antd-admin.git",
  "author": "Alex Woodman <alexthegoodman@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:pre; yarn build:client; yarn build:server;",
    "build:pre": "rm -rf dist; gulp image;",
    "build:client": "better-npm-run build:client;",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json './modules/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix -c ./tslint.json -p ./tsconfig.json './modules/**/*.{ts,tsx}'",
    "fmt:prettier": "prettier --write 'modules/**/*.{ts,tsx,js,jsx}'",
    "fmt": "yarn fmt:prettier; yarn lint:fix",
    "pretest": "yarn lint",
    "test": "yarn test:unit && yarn test:ui",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:unit:ci": "cross-env NODE_ENV=test jest --maxWorkers 8",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:ui": "cypress run",
    "gen:ui": "bash/gen_component.sh",
    "build-storybook": "build-storybook"
  },
  "betterScripts": {
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.config.js --mode=production",
    "build:client:watch": "NODE_ENV=production webpack --config ./webpack/client.config.js --watch --mode=production"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@sentry/browser": "^6.1.0",
    "@types/node": "^14.14.25",
    "antd": "^4.12.2",
    "babel-loader": "^8.2.2",
    "better-npm-run": "^0.1.1",
    "change-case": "^4.1.2",
    "compression-webpack-plugin": "^7.1.2",
    "config": "^3.3.3",
    "copy-webpack-plugin": "^7.0.0",
    "dotenv-webpack": "^6.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "formik": "^2.2.6",
    "global": "^4.4.0",
    "good-helpers": "https://github.com/alexthegoodman/good-helpers",
    "good-ui": "https://github.com/alexthegoodman/good-ui",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "happypack": "^5.0.1",
    "he": "^1.2.0",
    "html-webpack-plugin": "^5.0.0",
    "html-webpack-prerender-plugin": "^0.1.0",
    "icon-font-loader": "^3.0.2",
    "imagemin-webpack-plugin": "^2.4.2",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.5",
    "mixpanel-browser": "^2.41.0",
    "navi": "^0.14.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react": "^17.0.1",
    "react-apollo": "^3.1.5",
    "react-cookie": "^4.0.3",
    "react-data-table-component": "^7.0.0-alpha-5",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-hot-loader": "^4.13.0",
    "react-mixpanel": "^1.0.5",
    "react-navi": "^0.14.4",
    "react-select": "^4.0.2",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0",
    "yup": "^0.32.8"
  }
}
